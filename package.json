{
  "name": "vue-analytics",
  "version": "0.0.0-development",
  "description": "Google Analytics plugin for Vue",
  "repository": {
    "type": "git",
    "url": "https://github.com/MatteoGabriele/vue-analytics"
  },
  "scripts": {
    "commit": "git-cz",
    "clean": "del-cli dist",
    "prebuild": "yarn clean",
    "build": "bili",
    "dev": "bili --config bili.config.dev.js --watch",
    "test": "jest",
    "prepublish": "yarn build",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "keywords": [
    "Google",
    "google",
    "Google Analytics",
    "google analytics",
    "tracking",
    "google tracking",
    "vue-analytics",
    "vue"
  ],
  "main": "./dist/vue-analytics.js",
  "module": "./dist/vue-analytics.esm.js",
  "unpkg": "./dist/vue-analytics.umd.js",
  "jsdelivr": "./dist/vue-analytics.umd.js",
  "types": "vue-analytics.d.ts",
  "files": [
    "dist",
    "vue-analytics.d.ts"
  ],
  "author": {
    "name": "Matteo Gabriele",
    "email": "m.gabriele.dev@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MatteoGabriele/vue-analytics/issues"
  },
  "homepage": "https://github.com/MatteoGabriele/vue-analytics#readme",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "bili": "^4.8.1",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "del-cli": "^3.0.0",
    "jest": "^24.9.0",
    "semantic-release": "^15.13.30",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  }
}
